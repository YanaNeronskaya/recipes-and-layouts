            font-size 2em
            font-family Bello, sans-serif
            color #FD926D
          @media (max-width: 1199px)
            font-size 1em
            font-family Bello, sans-serif
            color #FD926D
        &-party
          @media (min-width: $desktop)
            font-size 2.7em
            font-family Helvetica-UL
            color #C1AEA8
            font-weight: lighter;
          @media (max-width: 1199px)
            font-size: 1.7em;
            font-family Helvetica-UL
            color #C1AEA8
            font-weight: lighter;
        &-collection
          @media (min-width: $desktop)
            font-size 1em
            font-family: Helvetica-BdCn;
            color: #6a6a6a;
          @media (max-width: 1199px)
            font-size: 0.8em;
            font-family: Helvetica-BdCn;
            color: #6a6a6a;
    &__context
      position absolute
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      margin-top: -30%;
      margin-left: 20%;
  &__container
    display flex;
    flex-grow 1
    cursor pointer
    &-rotated
      display: flex;
      flex-direction: column;

.ad-big-text
  background: none;
  position absolute
  font-family: Helvetica-LtCn, sans-serif;
  font-size: 28px;
  color: #78645d;
  &-bold
    font-family Helvetica-Bl, sans-serif
    font-weight bold
    font-size 2em
    color #414141
  &-first
    font-family Helvetica-ThCn
    font-size 3em
    color #414141
    letter-spacing: 0.1em;
  &-title
    background-image url("../img/&.png");
    width: 1.4em
    height 1.4em
    &::before
      position: absolute;
      margin: 9% 0% 0px 22%;
      height: 0.5%;
      content: " ";
      text-shadow: none;
      background-color: #414141;
      width: 35%;
    &::after
      position: absolute;
      margin: 9% 0px 0 -38%;
      height: 0.5%;
      content: " ";
      text-shadow: none;
      background-color: #414141;
